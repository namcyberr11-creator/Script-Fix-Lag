local Q=game:GetService("RunService")local s=game:GetService("CoreGui")local q=Instance.new("ScreenGui")q.Name="VuKhoaFixLagUI"q.Parent=s local m=Instance.new("TextLabel")m.Parent=q m.BackgroundTransparency=1 m.Position=UDim2.new(.5,-150,.1,0)m.Size=UDim2.new(0,300,0,70)m.Font=Enum.Font.SourceSansBold m.TextSize=35 m.TextColor3=Color3.fromRGB(0,0,0)m.TextStrokeColor3=Color3.fromRGB(255,255,255)m.TextStrokeTransparency=0 local D=0 Q.RenderStepped:Connect(function(Q)if tick()-D>=.5 then local s=math.floor(1/Q)m.Text="VuKhoa FixLag\nFPS: "..s D=tick()end end)local function E()(game:GetService("Lighting")).GlobalShadows=false;(settings()).Rendering.QualityLevel=1 for Q,s in pairs(workspace:GetDescendants())do if s:IsA("BasePart")and not s:IsDescendantOf(game.Players.LocalPlayer.Character)then if s.Name=="Tree"or s.Name=="Leaf"or s.Name=="Bush"then s:Destroy()elseif s:IsA("MeshPart")or s:IsA("UnionOperation")then s.Transparency=1 end end end end spawn(function()while wait(2)do for Q,s in pairs((game:GetService("ReplicatedStorage")):GetChildren())do if s.Name=="DamageIndicator"then s:Destroy()end end for Q,s in pairs(workspace:GetDescendants())do if s:IsA("ParticleEmitter")or s:IsA("Trail")then s.Enabled=false end if s.Name=="Attack"or s.Name=="Effect"then s:Destroy()end end end end)E()print("VuKhoa FixLag: Black & White Theme Loaded!")
